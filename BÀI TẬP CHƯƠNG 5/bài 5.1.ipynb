   "source": [
    "def print_sqlite_version():\n",
    "    conn=sqlite3.connect('example.db')\n",
    "    cursor=conn.cursor()\n",
    "    cursor.execute('SELECT sqlite_version()')\n",
    "    version=cursor.fetchone()\n",
    "    print(f\"SQLite Version: {version[0]}\")\n",
    "    conn.close()\n",
    "print_sqlite_version()\n"
   ]
  }
